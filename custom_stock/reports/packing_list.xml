                                            <t t-set="net_weight" t-value="o.move_ids_without_package[start].product_uom_qty *  o.move_ids_without_package[start].product_id.weight"/>
                                            <t t-set="gross_weight" t-value="o.move_ids_without_package[start].product_uom_qty *  o.move_ids_without_package[start].product_id.gross_weight"/>
                                            <tr style="border-right:1px solid black; border-left:1px solid black; height:50px !important;" >
                                                <td style="border-right:1px solid black;" class="text-left">
                                                    <span t-field="o.move_ids_without_package[start].product_id.product_tmpl_id.x_studio_many2one_field_UqhIy.x_name"/>
                                                </td>
                                                <td style="border-right:1px solid black;">
                                                    <span t-field="o.move_ids_without_package[start].product_id.display_name"/><br/>
                                                    <span t-field="o.move_ids_without_package[start].product_id.description_picking"/>
                                                </td>
                                                <td style="border-right:1px solid black;" class="text-right">
                                                    <span t-if="o.state != 'done'" t-esc="'{0:,.2f}'.format(o.move_ids_without_package[start].product_uom_qty)"/>
                                                    <span t-if="o.state == 'done'" t-esc="'{0:,.2f}'.format(o.move_ids_without_package[start].product_uom_qty)"/>
                                                    <span t-field="o.move_ids_without_package[start].product_uom" groups="uom.group_uom" style="margin-right:1px;"/>
                                                </td>
                                                <td style="border-right:1px solid black;" class="text-right">
                                                    <span t-esc="'{0:,.2f}'.format(o.move_ids_without_package[start].product_id.weight)" style="margin-right:1px;"/>
                                                </td>
                                                <td style="border-right:1px solid black;" class="text-right">
                                                    <span t-esc="'{0:,.2f}'.format(net_weight)" style="margin-right:1px;"/>
                                                </td>
                                                <td style="border-right:1px solid black;" class="text-right">
                                                    <span t-esc="'{0:,.2f}'.format(gross_weight)" style="margin-right:1px;"/>
                                                </td>
                                            </tr>
                                            <t t-set="sum_total" t-value="sum_total + o.move_ids_without_package[start].product_uom_qty"/>
                                            <t t-set="sum_weight" t-value="sum_weight + o.move_ids_without_package[start].product_id.weight"/>
                                            <t t-set="uom_name" t-value="o.move_ids_without_package[start].product_uom.name"/>
                                            

                                            <t t-set="sum_net_weight" t-value="sum_net_weight + net_weight"/>
                                            <t t-set="sum_gross_weight" t-value="sum_gross_weight + gross_weight"/>
                                            <t t-set="net_weight" t-value="1"/>
                                            
                                        </t>
                                        <t t-if="start == total_item-1">
                                            <t t-set="start" t-value="total_item-1"/>
                                            <t t-set="end" t-value="total_item-1"/>
                                        </t>
                                        <t t-else="">
                                            <t t-set="start" t-value="start + 1"/>
                                        </t>
                                    </t>
                                </tbody>
                                <tfoot>
                                    <tr style="border:1px solid black;">
                                        <t t-if="o.partner_id.lang =='en_US'">
                                            <td colspan="2" class="text-center" style="border-right:1px solid black;"><span>Total</span></td>
                                        </t>
                                        <t t-else="">
                                            <td colspan="2" class="text-center" style="border-right:1px solid black;"><span>รวมทั้งสิ้น</span></td>
                                        </t>
                                        
                                        <td style="border-right:1px solid black;margin-right:1px;" class="text-right"><span t-esc="'{0:,.2f}'.format(sum_total)"/>&amp;nbsp;<span t-esc="uom_name"/></td>
                                        <td style="border-right:1px solid black;margin-right:1px;" class="text-right"><span t-esc="'{0:,.2f}'.format(sum_weight)"/></td>
                                        <td style="border-right:1px solid black;margin-right:1px;" class="text-right"><span t-esc="'{0:,.2f}'.format(sum_net_weight)"/></td>
                                        <td style="border-right:1px solid black;margin-right:1px;" class="text-right"><span t-esc="'{0:,.2f}'.format(sum_gross_weight)"/></td>
                                    </tr>
                                    <tr style="border:1px solid black;">
                                        <td class="text-center" colspan="6" style="border-right:1px solid black;">For Bluefalo company Limited</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">
                                            <t t-if="o.partner_id.lang == 'en_US'">
                                                <span>&amp;nbsp;&amp;nbsp;Approved By:</span><br/><br/><br/>
                                            </t>
                                            <t t-else="">
                                                <span>&amp;nbsp;&amp;nbsp;อนุมัติโดย:</span><br/><br/><br/>
                                            </t>
                                            
                                            <span>&amp;nbsp;&amp;nbsp;..........................................................</span><br/>
                                            <!-- <span>&amp;nbsp;&amp;nbsp;</span><span t-esc="o.sale_id.user_id.name"/><br/> -->
                                            <span>&amp;nbsp;&amp;nbsp;</span><br/>
                                            <span>&amp;nbsp;&amp;nbsp;</span><span >Sales Executive</span><br/>
                                            <t t-if="o.partner_id.lang == 'en_US'">
                                                <span>&amp;nbsp;&amp;nbsp;</span><span t-esc="o.company_id.company_name_en"></span><br/>
                                            </t>
                                            <t t-else="">
                                                <span>&amp;nbsp;&amp;nbsp;</span><span t-esc="o.company_id.name"></span><br/>
                                            </t>
                                        </td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td ></td>
                                    </tr>
                                </tfoot>
                                
                            </table>
                        </t>
                    </div>
                    <t t-set="current_page" t-value="current_page+1"/>

                    </t>
                    </t>
                </t>
        </t>
    </template>
    <record id="stock_packing_list_report" model="ir.actions.report">
      <field name="name">Packing List</field>
