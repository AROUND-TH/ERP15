      <field name="paperformat_id" ref="custom_stock.paper_format_A4_zero_margin"/>
      <field name="report_name">custom_stock.stock_packing_list_document_multi_lang</field>
      <field name="report_file">custom_stock.stock_packing_list_document_multi_lang</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
      <field name="binding_type">report</field>
    </record>
  </data>

